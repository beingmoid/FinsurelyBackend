{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://192.168.18.7:5000/",
      "sslPort": 0
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "weatherforecast",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "PanoramaBackend": {
      "commandName": "Project", 
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://192.168.18.7:5000/"
    },
    "Prod": {
      "commandName": "Project",
      "launchUrl": "swagger",
      "environmentVariables": {
        "Commercial": "",
        "ASPNETCORE_ENVIRONMENT": "Production"
      },
      "externalUrlConfiguration": true,
      "applicationUrl": "http://192.168.18.7:5000/"
    },
    "WSL": {
      "commandName": "WSL2",
      "launchBrowser": true,
      "launchUrl": "http://192.168.18.7:5000/",
      "environmentVariables": {
        "ASPNETCORE_URLS": "http://192.168.18.7:5000/",
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "distributionName": ""
    },
    "Docker": {
      "commandName": "Docker",
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/weatherforecast",
      "publishAllPorts": true
    }
  }
}